#!/usr/bin/make -f
# -*- makefile -*-

override_dh_autoreconf:
	sed -i -e 's/lua >= 5.1.0/lua51/' configure.ac
	sed -i 's|<\(l[ua][ua][^>]*\)>|<lua5.1/\1>|' src/composite-metrics.cc
	dh_autoreconf

override_dh_strip:
	dh_strip --dbg-package=composite-metrics-dbg

override_dh_auto_test:
	echo "Skipped for now"
%:
	dh $@ --with=autoreconf

.PHONY: override_dh_strip override_dh_auto_test
